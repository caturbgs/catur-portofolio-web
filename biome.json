{
  "$schema": "https://biomejs.dev/schemas/1.9.2/schema.json",
  "extends": ["/Users/caturbgs/code/biome.json"],
  "overrides": [
    {
      "include": ["*.svelte", "*.astro", "*.vue"],
      "linter": {
        "rules": {
          "style": {
            "useConst": "off",
            "useImportType": "off"
          },
          "correctness": {
            "noUnusedFunctionParameters": "error",
            "noUnusedImports": "off",
            "noUnusedVariables": "off"
          }
        }
      },
      "javascript": {
        "formatter": {
          "quoteStyle": "double"
        }
      }
    }
  ]
}
